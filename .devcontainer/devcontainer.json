{
    "name": "Databases training",
    "dockerComposeFile": "docker-compose.yml",
    "service": "devcontainer",
    "forwardPorts": [3306, 9200, 9600, 3000],
    "portsAttributes": {
      "3306": {
        "label": "MySQL",
        "onAutoForward": "notify"
      },
      "9200": {
        "label": "OpenSearch",
        "onAutoForward": "notify"
      },
      "9600": {
        "label": "OpenSearch",
        "onAutoForward": "notify"
      },
      "3000": {
        "label": "Grafana",
        "onAutoForward": "notify"
      }
    },
    "postCreateCommand": "bash .devcontainer/setup.sh"
}
